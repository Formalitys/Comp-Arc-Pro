v0xaaaaecde34f0_0 .net "right_shift", 3 0, L_0xaaaaecde3cd0;  alias, 1 drivers
L_0xaaaaecde3980 .part v0xaaaaecde3650_0, 0, 3;
L_0xaaaaecde3aa0 .concat [ 1 3 0 0], L_0xffffba9c6018, L_0xaaaaecde3980;
L_0xaaaaecde3c30 .part v0xaaaaecde3650_0, 1, 3;
L_0xaaaaecde3cd0 .concat [ 3 1 0 0], L_0xaaaaecde3c30, L_0xffffba9c6060;
    .scope S_0xaaaaecdce020;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xaaaaecde3710_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0xaaaaecdce020;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0xaaaaecde3710_0;
    %nor/r;
    %store/vec4 v0xaaaaecde3710_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0xaaaaecdce020;
T_2 ;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0xaaaaecde3650_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0xaaaaecde3650_0, 0, 4;
